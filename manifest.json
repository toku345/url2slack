{
  "manifest_version": 2,

  "name": "url2slack",
  "description": "Send `url` to your slack channel!",
  "version": "0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "activeTab",
    "https://*.slack.com/"
  ]
}
